{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    "@babel/preset-stage-3",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
